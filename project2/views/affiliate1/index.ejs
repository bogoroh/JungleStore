<!-- Default home page -->
<link type="text/css" href='/styles/affiliate1style.css' rel='stylesheet'/>

<!--[if IE 7]>
    <style>
    .getting-started li{overflow:visible;clear:both}.delete{width:690px}
    </style>
<![endif]-->


<div class="container clearfix">
    <div class="main">

        <h1>Affiliate 1</h1>
        <% include nav %>

        <% for(var i=0; i<details.length; i++) {%>

           	<form action="/addcart?sku=<%= details[i].sku %>&price=<%= prices[i].price %>&name=<%= details[i].name %>" method="POST">
                
                <p>Store Price:</p><p name="price"><%= prices[i].price %></p>
                <p>SKU: </p><p name="sku"><%= details[i].sku %></p>
                <p>Name: </p><p name="itemName"><%= details[i].name %></p>
                <p>MSRP: </p><p><%= details[i].msrp %></p>
                <p>Whole Sale: </p><p><%= details[i].wholesale %></p>
                <p>Category: </p><p><%= details[i].category %></p>
                <p></p><p><%= details[i].stock %></p>
                <% for(var j=0; j<details[i].photos.length; j++) {%>
                   <img src="/images/<%= details[i].photos[j] %>.jpg">
                <% } %>
                <input type="submit" value="Add To Cart"/>

            </form>

        <% } %>




    </div>

</div>

