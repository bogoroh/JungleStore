<div class="container clearfix">
    <div class="main">

        <h1>Affiliate 2</h1>
        <% include nav %>



           <p>

                store price: <%= prices.price %><br>


                sku: <%= details.sku %><br>
                product: <%= details.name %><br>
                msrp: <%= details.msrp %><br>
                wholesale: <%= details.wholesale %><br>
                category: <%= details.category %><br>
                stock: <%= details.stock %><br>
                <% for(var j=0; j<details.photos.length; j++) {%>
                <a href="/images/<%= details.photos[j] %>" data-lightbox="productImages"><img src="/images/<%= details.photos[j] %>"></a>

                <% } %>



            </p>






    </div>

</div>
